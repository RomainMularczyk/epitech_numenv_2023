---
import { getCollection } from "astro:content";
import PageLayout from "@/layouts/Base";
import Subscribe from "@/components/subscribe/Subscribe";
import Form from "../../components/subscribe/Form.svelte";
import Title from "@/components/subscribe/FormTitle";
export const prerender = true;

export async function getStaticPaths() {
  const programEntries = await getCollection("program");

  return programEntries.map(entry => ({
    params: { entry: entry.slug }, props: { entry }
  }));
}

const { entry } = Astro.props;
const meta = {
  title: "Inscription",
  data: entry.data,
  description: `Inscription pour la session ${entry.data.title}`,
}
---

<PageLayout meta={meta}>
  <Title data={meta.data} />
  <Subscribe data={meta.data} />
</PageLayout>

